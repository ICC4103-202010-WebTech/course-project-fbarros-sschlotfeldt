<div class="row ml-0">
<div class="card">
  <div class="card-body">
    <h5 class="card-title"><%= date_section_title %></h5>
    <p><%= date_instruction_text %></p>
    <%= form_with model: @vote, local: true do |date_form| %>
      <div id="ticket-type-nested-forms" class="form-group">
        <%= date_form.date_field :date, class: "form-control" %>
        <hr>
      </div>
      <%= date_form.submit submit_btn_text, class: ["btn", "btn-primary"] %>
      <%= link_to "Back", :back, class: ["btn", "btn-secondary"] %>
  <% end %>
    <% if action_name == 'new' %>
      <%= link_to "Add Date", new_vote_path,
                  class: [ 'btn', 'btn-primary'], remote: true %>
    <% end %>
  </div>
</div>
<div class="form-group mt-2">

</div>
</div>