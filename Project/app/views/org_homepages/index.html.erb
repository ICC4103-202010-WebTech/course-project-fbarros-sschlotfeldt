<p id="notice"><%= notice %></p>

<h1>Org Homepages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @org_homepages.each do |org_homepage| %>
      <tr>
        <li><h5><%=link_to org_homepage.name, org_homepage_path(org_homepage.id)%></h5></li>
        <li><%= [org_homepage.name] %></li>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Org Homepage', new_org_homepage_path %>
