<div class="row ml-0">

  <%= form_for @org_homepage do |f| %>

    <div class="form-group">
      <%= f.label :Description %>
      <%= f.text_field :description %>
    </div>


    <div class="form-group">
      <%=f.label :Name %>
      <%=f.text_field :name %>
    </div>

    <div class="form-group mt-2">
      <%= f.submit :Update, class: ["btn", "btn-primary"] %>
      <%= link_to "Back", :back, class: ["btn", "btn-secondary"] %>
    </div>

  <%end %>

</div>